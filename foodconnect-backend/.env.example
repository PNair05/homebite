# Copy this file to .env and fill in values. Settings use the FC_ prefix.

# FastAPI app settings
FC_APP_NAME="FoodConnect Backend"
FC_DEBUG=true

# SQLite (default)
# FC_DATABASE_URL="sqlite:///./app.db"

# Postgres via TCP (psycopg3 driver) - replace with your values
# FC_DATABASE_URL="postgresql+psycopg://<USER>:<PASSWORD>@<HOST>:<PORT>/<DBNAME>"

# Postgres via Cloud SQL Auth Proxy over TCP (listening on localhost:5432)
# Start the proxy first, then use:
# FC_DATABASE_URL="postgresql+psycopg://<USER>:<PASSWORD>@127.0.0.1:5432/<DBNAME>"

# Postgres via Unix socket (Cloud SQL Auth Proxy) - macOS/Linux
# If proxy exposes /cloudsql/<INSTANCE_CONNECTION_NAME> socket path
# FC_DATABASE_URL="postgresql+psycopg://<USER>:<PASSWORD>@/<DBNAME>?host=/cloudsql/<PROJECT>:<REGION>:<INSTANCE>"

# Optional API keys
# FC_OPENAI_API_KEY=""
